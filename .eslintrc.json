{
  "extends": ["react-app", "plugin:jsx-a11y/recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "import/order": [
      "error",
      {
        "groups": ["external", "internal", "parent", "sibling", "index"],
        "newlines-between": "always",
        "alphabetize": {
          "caseInsensitive": true
        }
      }
    ],
    "import/no-anonymous-default-export": [
      "error",
      {
        "allowObject": true
      }
    ]
  }
}
